<?xml version="1.0" encoding="utf-8"?>

<resources>

    <style name="AutoTextViewForm" parent="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense.ExposedDropdownMenu">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">8dp</item>
        <item name="android:layout_marginEnd">8dp</item>
    </style>

    <style name="AutoTextViewVtrs" parent="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense.ExposedDropdownMenu">
        <item name="android:layout_width">400dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">8dp</item>
        <item name="android:layout_marginEnd">8dp</item>
    </style>


    <style name="textView_geral">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/cor_fonte</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_marginTop">16dp</item>
    </style>

    <style name="textView_geral2">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/cor_fonte</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_marginTop">32dp</item>
    </style>


    <style name="img_logo">
        <item name="android:layout_width">150dp</item>
        <item name="android:layout_height">65dp</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:layout_marginEnd">16dp</item>
    </style>

    <style name="btn_auxiliar">
        <item name="android:layout_width">120dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:background">@drawable/perfil_btn</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_marginEnd">8dp</item>
        <item name="android:layout_marginTop">16dp</item>
    </style>

    <style name="btn_principal">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:background">@drawable/perfil_btn</item>
        <item name="android:layout_marginStart">8dp</item>
        <item name="android:layout_marginEnd">8dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:enabled">false</item>
    </style>

    <style name="estilo_toolbar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/tom_azul1</item>
        <item name="android:minHeight">?attr/actionBarSize</item>
        <item name="android:theme">?attr/actionBarTheme</item>
    </style>

    <style name="textView_hint">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/cor_fonte</item>
        <item name="android:layout_marginStart">15dp</item>
        <item name="android:layout_marginTop">16dp</item>
    </style>

    <style name="view_horizontal">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_marginStart">8dp</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:layout_marginEnd">8dp</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>

    <style name="view_vertical">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">126dp</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>

    <style name="textView1_resumo">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/cor_fonte</item>
    </style>

    <style name="textView2_resumo">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">8dp</item>
        <item name="android:textColor">@color/cor_fonte</item>
        <item name="android:textSize">16sp</item>
    </style>


    <style name="textView3_resumo">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">8dp</item>
        <item name="android:textColor">@color/cor_fonte</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="linearLayout1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="linearLayout2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">24dp</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="descritivo1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">2dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_marginEnd">2dp</item>
        <item name="android:background">@drawable/perfil_input</item>
        <item name="android:padding">8dp</item>
        <item name="android:textColor">@color/cor_fonte</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="ckb">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:textColor">@color/cor_fonte</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="textInputLayout">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">8dp</item>
        <item name="android:layout_marginTop">24dp</item>
        <item name="android:layout_marginEnd">8dp</item>
        <item name="counterEnabled">true</item>
        <item name="counterMaxLength">400</item>
        <item name="errorEnabled">true</item>
        <item name="layout_constraintEnd_toEndOf">parent</item>
        <item name="layout_constraintStart_toStartOf">parent</item>
    </style>

    <style name="textInputEditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">top</item>
        <item name="android:inputType">textMultiLine</item>
    </style>

    <style name="autoComplete">
        <item name="android:layout_width">350dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:layout_marginStart">8dp</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:completionThreshold">1</item>
        <item name="android:textColor">@color/cor_fonte</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="editTextVitima">
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginTop">24dp</item>
        <item name="android:inputType">number</item>
        <item name="android:maxLength">2</item>
    </style>
</resources>